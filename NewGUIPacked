local Tab = Window:CreateTab("Weapons", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Weapons")

Section:Set("Weapons")

local Button = Tab:CreateButton({
    Name = "Get Corrupted Rifle",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/ukav/Folder/refs/heads/main/CorruptedRifle", true))()
    end,
})

local Button = Tab:CreateButton({
    Name = "Get LXD",
    Callback = function()
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local root = char:WaitForChild("HumanoidRootPart")
        local targetHead = game:GetService("Workspace"):WaitForChild("DeadGround"):WaitForChild("LXD - 9999 Cell"):WaitForChild("Head")
        firetouchinterest(root, targetHead, 0) -- touch began
        firetouchinterest(root, targetHead, 1) -- touch ended

    end,
})

local Button = Tab:CreateButton({
    Name = "Get Dual LX3",
    Callback = function()
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local root = char:WaitForChild("HumanoidRootPart")
        local targetHead = game:GetService("Workspace"):WaitForChild("MazeWorld"):WaitForChild("ESCAPE FROM THIS HELL"):WaitForChild("Head")
        firetouchinterest(root, targetHead, 0) -- touch began
        firetouchinterest(root, targetHead, 1) -- touch ended
    end,
})

local Button = Tab:CreateButton({
