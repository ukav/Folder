local Button = Tab:CreateButton({
    Name = "Get Water Gun",
    Callback = function()
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
        local root = char:WaitForChild("HumanoidRootPart")

        local targetPos = Vector3.new(624, 1090, 750.5)
        root.CFrame = CFrame.new(targetPos)
        task.wait(1.5)

        -- First prompt
        root.CFrame = CFrame.new(-443, 971, -196)
        task.wait(0.5)
        local player = game:GetService("Players").LocalPlayer

        local function getNearestPrompt()
            local char = player.Character
            if not char then return nil end
            local root = char:FindFirstChild("HumanoidRootPart")
            if not root then return nil end

            local nearest, shortest = nil, math.huge
            for _, v in pairs(workspace:GetDescendants()) do
                if v:IsA("ProximityPrompt") and v.Enabled then
                    local part = v.Parent:IsA("BasePart") and v.Parent or nil
                    if part then
                        local dist = (root.Position - part.Position).Magnitude
                        if dist < shortest then
                            shortest = dist
                            nearest = v
                        end
                    end
                end
            end
            return nearest
        end

        local prompt = getNearestPrompt()
        if prompt then
            prompt:InputHoldBegin(Enum.UserInputType.Keyboard)
            prompt:InputHoldEnd(Enum.UserInputType.Keyboard)
        end

        -- Second prompt
        root.CFrame = CFrame.new(-282, 971, -92)
        task.wait(0.5)

        prompt = getNearestPrompt()
        if prompt then
            prompt:InputHoldBegin(Enum.UserInputType.Keyboard)
            prompt:InputHoldEnd(Enum.UserInputType.Keyboard)
        end

        -- Touch Water Gun
        local targetPart = game:GetService("Workspace").Sewer.GetWaterGun
        firetouchinterest(root, targetPart, 0)
        firetouchinterest(root, targetPart, 1)
    end,
})

