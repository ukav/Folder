local Tab = Window:CreateTab("Armour", 4483362458) -- Title, Image

local Section = Tab:CreateSection("Armours")

Section:Set("Armours")

local Button = Tab:CreateButton({
   Name = "Get Blizzard Armor",
   Callback = function()
       local Players = game:GetService("Players")
       local LocalPlayer = Players.LocalPlayer
       local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
       local root = char:WaitForChild("HumanoidRootPart")
       local armorModel = workspace:WaitForChild("Misc"):WaitForChild("Blizzard Armor")
       local headPart = armorModel:WaitForChild("Head")
       headPart.CanCollide = false

       while char and char.Parent do
           firetouchinterest(headPart, root, 0)
           task.wait()
           firetouchinterest(headPart, root, 1)
           task.wait()
       end
   end,
})

